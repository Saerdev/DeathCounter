<Application x:Class="DeathCounter.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DeathCounter"
             StartupUri="MainWindow.xaml"
             DispatcherUnhandledException="Application_DispatcherUnhandledException">
    <Application.Resources>
        <FontFamily x:Key="Fantomen">pack://application:,,,/Resources/Fonts/#VFC Fantomen</FontFamily>
        <FontFamily x:Key="FantomenPress">pack://application:,,,/Resources/Fonts/#VFC Fantomen Press</FontFamily>
    </Application.Resources>
</Application>
